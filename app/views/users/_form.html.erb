<% @user.password = @user.password_confirmation = nil %>

<%= error_messages_for :user %>
<% form_for @user do |f| -%>

<%= f.text_field :login, :value => 'Username' %>


<%= f.text_field :email, :value => 'E-mail' %>

<%= f.text_field :billing_rate, :value => 'Billing Rates' %>


<%= f.password_field :password, :value => 'Password' %>


<%= f.password_field :password_confirmation, :value => 'Password' %>

<%= submit_tag @user.new_record? ? 'Sign up' : 'Update' %>
<% end -%>
